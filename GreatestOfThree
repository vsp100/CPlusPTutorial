#include<stdio.h>
#include<iostream>

using namespace std;

int main()
{
    int a , b, c;
    
    a = 25, b = 25, c = 25;
    
    cout<<"Greatest of three numbers is : ";
    
    if(a >= b && a >= c)
    cout<<a<<endl;
    else if( b >= a && b >= c)
    cout <<b<<endl;
    else
    cout<<c<<endl;
    
    return 0;
}
